const persona = {
    nombre: "Germán";
    edad: "42"
    profesión: "Desarrollador"
    gustos: ["Motos", "Literatura"]
};

console.log(persona.nombre);
console.log(persona[edad]);

persona.edad = 85;

persona.gustos.push["Poesía"];
console.log(persona[edad]);

//Ver elementos dentro de otros, como listas en diccionarios, o diccionarios en listas
//o diccionarios en otros diccionarios

console.log(persona);

persona.pais = "Colombia";

delete persona.pais;

console.log(Object.keys(persona));

//Usar (), [alt+91] y {alt+123} de a cuerdo al tipo de estructura
//El array es similar a una lista
//corchetes [ ] para listas o arrays, y llaves { } para diccionarios

const persona = {
    nombre: "Vicky",
    edad: 40,
    profesion: "Desarrolladora",
    gustos: ["cine", "lectura"]
};
console.log(persona);
persona.gustos.push("cocina");
console.log(persona);
persona.estatura = 161;
console.log(persona);
console.log(Object.keys(persona));

//Ver: parámetros y argumentos
//Ver: Hoisting

